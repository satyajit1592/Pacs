                  <form action="../../bankModule/bankDetails/asso_bank_deposit_returns_srch" method="post">

                    <!-- flash message -->
                    <% if(locals.message){ %>
                    <div class="text text-center" role="alert" background-color: red;>
                      <strong style="color:green"><%= message.success_msg %></strong>
                    </div>
                    <% } %>

                    <div class="container row mt-2">
                      <div class="col-sm-12 col-md-6">
                        <label for="ser_ass_bnk_depmas" class="mr-sm-4 col-sm-12 col-md-5"
