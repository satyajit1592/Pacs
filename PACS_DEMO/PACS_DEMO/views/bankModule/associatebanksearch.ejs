                  <h6>SEARCH</h6>
                  <hr>
                  <!-- table module -->
                  <div class="content">
                    <form action="/bankModule/bankDetails/search_particular_rec" method="post">
                      <div class="container row mb-4">
                        <div class="col-sm-12 col-md-6">
                          <label for="member_typ" class="mr-sm-4 col-sm-4" style="padding-left:0% !important;">Bank Code</label>
