                  <input type="button" class="btn print1" id="notif" value="Print" onclick="resetform()">
                </div>




                <div id="dtBasicExample_wrapper table-responsive" class="dataTables_wrapper dt-bootstrap4">

                  <div class="row">
                    <div class="col-sm-12 table-responsive form-inlinee">
                      <table id="dtBasicExample" class="table table-bordered table-sm dataTable" cellspacing="0"
                        width="100%" role="grid" aria-describedby="dtBasicExample_info" style="width: 100%;">
                        <thead>
                          <tr role="row">
                            <th class="th-sm sorting_asc" tabindex="0" aria-controls="dtBasicExample" rowspan="1"
                              colspan="1" aria-sort="ascending" aria-label="Name
                                                                        : activate to sort column descending"
                              style="width: 134.2px;">GL Code

                            </th>
                            <th class="th-sm sorting" tabindex="0" aria-controls="dtBasicExample" rowspan="1"
                              colspan="1" aria-label="Position
                                                                        : activate to sort column ascending"
                              style="width: 211.2px;">Account Number

                            </th>
                            <th class="th-sm sorting" tabindex="0" aria-controls="dtBasicExample" rowspan="1"
                              colspan="1" aria-label="Office
                                                                        : activate to sort column ascending"
                              style="width: 94.2px;">Bank Deposit Type

                            </th>
                            <th class="th-sm sorting" tabindex="0" aria-controls="dtBasicExample" rowspan="1"
                              colspan="1" aria-label="Age
                                                                        : activate to sort column ascending"
                              style="width: 89.2px;">Bank Deposit Number

                            </th>
                            <th class="th-sm sorting" tabindex="0" aria-controls="dtBasicExample" rowspan="1"
                              colspan="1" aria-label="Start date
                                                                        : activate to sort column ascending"
                              style="width: 93.2px;">Opening Balance

                            </th>
                            <th class="th-sm sorting" tabindex="0" aria-controls="dtBasicExample" rowspan="1"
                              colspan="1" aria-label="Start date
                                                                        : activate to sort column ascending"
                              style="width: 93.2px;">Current Balance

                            </th>

                            <th class="th-sm sorting" tabindex="0" aria-controls="dtBasicExample" rowspan="1"
                              colspan="1" aria-label="Salary
                                                                        : activate to sort column ascending"
                              style="width: 90.2px;">Actions
                            </th>
                          </tr>
                        </thead>
                        <tbody>
                          <% for(var i=0;i < depositdetails.length; i++) { %>

                          <tr id="rowid<%= depositdetails[i].abdm_ch_bank_deposit_id%>" role="row" class="odd">

                            <input type="radio" hidden name="radiobutton1bred"
                              id="radiobutton1bred<%=depositdetails[i].abdm_ch_bank_deposit_id%>"
                              value="<%=depositdetails[i].abdm_ch_bank_deposit_id%>" class="edit" />
                            <input type="radio" hidden name="radiobutton1rfvw"
                              id="radiobutton1rfvw<%=depositdetails[i].abdm_ch_bank_deposit_id%>"
                              value="<%=depositdetails[i].abdm_ch_bank_deposit_id%>" class="edit" />
                            <input type="radio" hidden name="radiobutton1rfdel"
                              id="radiobutton1rfdel<%=depositdetails[i].abdm_ch_bank_deposit_id%>"
                              value="<%=depositdetails[i].abdm_ch_bank_deposit_id%>" class="edit" />

                            <input type="text" name="deposit_id<%= depositdetails[i].abdm_ch_bank_deposit_id %>"
                              id="deposit_id<%= depositdetails[i].abdm_ch_bank_deposit_id %>"
                              value="<%=depositdetails[i].abdm_ch_bank_deposit_id %>" hidden>
                            <input type="text" name="deposit_id1<%= depositdetails[i].abdm_ch_bank_deposit_id %>"
                              id="deposit_id1<%= depositdetails[i].abdm_ch_bank_deposit_id %>"
                              value="<%=depositdetails[i].abdm_ch_bank_deposit_id %>" hidden>
                            <input type="text" name="deposit_id2<%= depositdetails[i].abdm_ch_bank_deposit_id %>"
                              id="deposit_id2<%= depositdetails[i].abdm_ch_bank_deposit_id %>"
                              value="<%=depositdetails[i].abdm_ch_bank_deposit_id %>" hidden>





                            <td class="sorting_1">
                              <%= depositdetails[i].abdm_n_gl_code %>
                            </td>
                            <td>
                              <%= depositdetails[i].abdm_n_acct_num %>
                            </td>
                            <td>
                              <%= depositdetails[i].abdm_ch_bank_deposit_type %>
                            </td>
                            <td>
                              <%= depositdetails[i].abdm_n_bank_deposit_num %>
                            </td>
                            <td>
                              <%= depositdetails[i].abdm_n_opening_bal %>
                            </td>
                            <td>
                              <%= depositdetails[i].abdm_n_cur_bal %>
                            </td>

                            <td><input type="text" name="brfed<%=depositdetails[i].abdm_ch_bank_deposit_id%>"
                                id="brfed<%=depositdetails[i].abdm_ch_bank_deposit_id%>"
                                value="<%=depositdetails[i].abdm_ch_bank_deposit_id %>" hidden><i style="font-size:16px"
                                class="fa" id="ed<%= depositdetails[i].abdm_ch_bank_deposit_id %>"
                                onclick="edit_deposit_row(this)">&#xf044;</i>
                              <input type="text" name="vfvw<%=depositdetails[i].abdm_ch_bank_deposit_id%>"
                                id="vfvw<%=depositdetails[i].abdm_ch_bank_deposit_id%>"
                                value="<%=depositdetails[i].abdm_ch_bank_deposit_id %>" hidden>
                              <i style="font-size:16px" class="fa"
                                id="vw<%= depositdetails[i].abdm_ch_bank_deposit_id %>"
                                onclick="view_deposit_row(this)">&#xf06e;</i>
                              <input type="text" name="dfdel<%=depositdetails[i].abdm_ch_bank_deposit_id%>"
                                id="dfdel<%=depositdetails[i].abdm_ch_bank_deposit_id%>"
                                value="<%=depositdetails[i].abdm_ch_bank_deposit_id %>" hidden>
                              <i style="font-size:16px" class="fa"
                                id="del<%= depositdetails[i].abdm_ch_bank_deposit_id %>"
                                onclick="delete_deposit_row(this)">&#xf2ed;</i>
                            </td>
                          </tr>


                          <% } %>
                        </tbody>


                      </table>
                    </div>
                  </div>

                </div>

                <br>


                <div class="clearfix"></div>

              </div>
            </div>
          </div>
