              <div class="card-header">

              </div>
              <div class="card-body">
                <div class="content">
                  <h6>Associated Bank Deposit Master</h6>
                  <!-- flash message -->
                  <% if(locals.message){ %>
                  <div class="text text-center" role="alert" background-color: red;>
                    <strong style="color:green"><%= message.success_msg %></strong>
                  </div>
                  <% } %>
                  <hr>
                  <h6>Search</h6>
                  <hr>

                  <form action="/bankModule/bankDetails/search_particular_deposit_rec" method="post">

                    <div class="container row mt-2">
                      <div class="col-sm-12 col-md-6">
                        <label for="gl_cd" class="mr-sm-4 col-sm-12 col-md-5" style="padding-left:0% !important;">GL
                          Code</label>
                        <input type="number" class="col-sm-12 col-md-6" name="gl_cd" id="gl_cd" placeholder="">

                      </div>
                      <div class="col-sm-12 col-md-6">
                        <label for="ac_num" class="mr-sm-4 col-sm-12 col-md-5"
                          style="padding-left:0% !important;">Account Number
                        </label>
