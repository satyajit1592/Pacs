                        <input type="number" class="col-sm-12 col-md-6" name="bnk_dep_num" id="bnk_dep_num"
                          placeholder="">

                      </div>
                    </div>
                </div>


                <input type="submit" name="btnUplEmp" id="btnUplEmp" class="btn" value="Search"
